<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle"
            android:padding="20dp"
            >
            <corners
                android:topLeftRadius="54dp"
                android:topRightRadius="54dp"
                android:bottomLeftRadius="54dp"
                android:bottomRightRadius="54dp"
                />
            <solid
                android:color="#4F4F4F"
                />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp"
                />
            <size
                android:width="60dp"
                android:height="60dp"
                />
            <stroke
                android:width="3dp"
                android:color="#EBEB2F"
                />
        </shape>
    </item>
    <item android:drawable="@drawable/wellness" />
</layer-list>
